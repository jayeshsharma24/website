pipeline {
    agent any
   
    stages {
        stage('Git checkout') {
            steps {
                git branch: 'web', url: 'https://github.com/jayeshsharma24/website.git'
            }
        }
        stage('Build') {
            steps {
                sh """
                    echo '--------------------building stage--------------------'
                                   """
                sh 'mvn clean install -DskipTests'
            }
            post {
                success {
                    echo 'Now Archiving...'
                    archiveArtifacts artifacts: '**/target/*.war'
                }
            }
        

            }
        
      stage('third-stage') {
            steps {
                sh """
                    echo 'third stage stage'
                """
            }
        }
        stage('fourth-stage') {
            steps {
                sh """
                    echo ''
                """
            }
        }
    }
}
